            @Html.DisplayFor(modelItem => item.CantMax)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CantTotal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
